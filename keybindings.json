// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+enter",
    "command": "editor.action.revealDefinition",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+down",
    "command": "cursorMove",
    "when": "textInputFocus",
    "args": {
      "to": "down",
      "by": "line",
      "value": 5
    }
  },
  {
    "key": "ctrl+up",
    "command": "cursorMove",
    "when": "textInputFocus",
    "args": {
      "to": "up",
      "by": "line",
      "value": 5
    }
  },
  {
    "key": "ctrl+shift+[",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "ctrl+shift+]",
    "command": "workbench.action.nextEditor"
  }
]
